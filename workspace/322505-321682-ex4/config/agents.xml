<?xml version="1.0" encoding="ISO-8859-1"?>

<agents>
    <!-- A list of agents and their behavior classes -->

    <!-- #### CENTRALIZED AGENTS ### -->
    <agent name="centralized-random">
        <set class-path="bin/"/>
        <set class-name="agents.CentralizedTemplate"/>
    </agent>

    <agent name="centralized-main">
        <set class-path="bin/"/>
        <set class-name="agents.CentralizedMain"/>
        <set p="0.4"/>
        <set alpha="4.0"/>
        <set beta="0.4"/>
<!--        <set alpha="10.0"/>-->
<!--        <set beta="0.4"/>-->
        <!-- fairBasedOnHomeCity or allTasksToBiggestVehicle -->
        <set initialSolution="fairBasedOnHomeCity"/>
<!--        <set initialSolution="allTasksToBiggestVehicle"/>-->
<!--        <set initialSolution="createInitialGiveAll"/>-->
    </agent>

</agents>
