<?xml version="1.0" encoding="ISO-8859-1"?>

<agents>
    <!-- A list of agents and their behavior classes -->

    <!-- #### AUCTION AGENTS ### -->
    <agent name="auction-random">
        <set class-path="bin/"/>
        <set class-name="agents.AuctionTemplate"/>
    </agent>

    <agent name="auction-main">
        <set class-path="bin/"/>
        <set class-name="agents.AuctionMain"/>

        <set epsilon="0.3"/>
		<set discount="0.5"/>
        <set probabilityThreshold="0.2"/>
        <set distributionDiscount="0.25"/>
	</agent>

</agents>
