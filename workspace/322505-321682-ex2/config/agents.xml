<?xml version="1.0" encoding="ISO-8859-1"?>

<agents>
	<!-- A list of agents and their behavior classes -->
	
	<!-- #### REACTIVE AGENTS ### -->
	<agent name="reactive-random">
		<set class-path="bin/"/>
		<set class-name="agents.ReactiveTemplate"/>
		<set discount-factor=".85"/>
	</agent>

	<agent name="reactive-reinforcement_learning">
		<set class-path="bin/"/>
		<set class-name="agents.ReactiveAgent"/>
		<set discount-factor=".8"/>
		<set epsilon="0.0001"/>
	</agent>
	
</agents>
